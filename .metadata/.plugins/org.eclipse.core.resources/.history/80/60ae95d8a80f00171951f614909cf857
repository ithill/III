package tw.leonchen.myproject.oop

class Fish{  //4.
	
	private String name = "Fish";
	
	public Fish(String name){
		super();   //3.
		this.name = name;  //5.
	}
	
	public void swim(){
		System.out.println("i can swim:" + name);
	}
}

class SwordFish extends Fish{
	
	public SwordFish(){
		super("");  //2.  //6.
	}
	
	public void speedup(){  //9.
		System.out.println("attack another fish and eat it.");
	}
}

public class TestSuperQ {

	public static void main(String[] args) {
		SwordFish swordFish = new SwordFish();  //1.  //7.
		swordFish.speedup();  //8.
		System.out.println("finished");  //10.
	}

}