package tw.leonchen.my.project.Equals;

class Animal{
	
	public void happy(){
		System.out.println("Animal is happy");
	}
}

class Cat extends Animal{
	
	public void happy(){
		System.out.println("mouw is happy");
	}
}
public class Polymorphism {

	public static void main(String[] args) {
		Animal a = new Cat();
		a.happy();
	}

}
