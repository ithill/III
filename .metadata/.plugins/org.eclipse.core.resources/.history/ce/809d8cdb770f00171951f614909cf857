
public class Person {
	int id;
	int age = 20;
	/*
	Person(int id,int age){
		this.id = id;
		this.age = age;
	}
	*/
	//系統默認的建構子為Person()
	
	public static void main(String[] args) {
		Person tom = new Person(1,25);//因為是默認的建構子,此行帶入實參馬上報錯
		System.out.println(tom.id);
		System.out.println(tom.age);
	}

}
