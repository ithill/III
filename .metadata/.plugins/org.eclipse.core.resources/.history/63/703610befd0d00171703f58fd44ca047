package tw.leonchen.my.project.oop;

public class TestOverload {
	void sum(int a,int b){
		System.out.println("a+b=" + (a+b));
	}
	void sum(double a,double b){
		System.out.println("a+b=" +(a+b));
	}
		
	
	public static void main(String[] args) {
		TestOverload t = new TestOverload(); 
		t.sum(3.3,5);
		t.sum('A',10);
	}
}
