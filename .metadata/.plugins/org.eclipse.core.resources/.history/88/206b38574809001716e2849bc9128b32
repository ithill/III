
public class TestOperator {

	public static void main(String[] args) {
		
		System.out.println("2&3=" +(2&3));
		System.out.println("2|3=" +(2|3));
		System.out.println("2^3=" +(2^3));
		System.out.println("~2=" +(~2));
		System.out.println("1234>>2=" +(1234>>2));
		System.out.println("-1234>>2=" +(-1234>>2));
	}
	

}
