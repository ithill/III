class Point{
	double x,y,z;
	Point(double x,double y,double z){
		this.x = x;
		this.y = y;
		this.z = z;
	}
	public void setX(double _x){
		x = _x;
	}
	public void setY(double _y){
		y = _y;
	}
	public void setZ(double _z){
		z = _z;
	}
	
}



public class TestPoint {

	public static void main(String[] args) {
		
	}

}
